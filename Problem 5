# Attempt 1

def change_green(filename):
    bit = Bit(filename)
    while bit.front_clear():
        bit.move()
        if bit.set_color()=='green':
            bit.paint('blue')
        elif bit.set_color()==None
            bit.paint('blue') 

# Attempt 2 - Add ":" on line 20

def change_green(filename):
    bit = Bit(filename)
    while bit.front_clear():
        bit.move()
        if bit.set_color()=='green':
            bit.paint('blue')
        elif bit.set_color()==None:
            bit.paint('blue') 

# Attempt 3 - Change "set" to "get" throughout

def change_green(filename):
    bit = Bit(filename)
    while bit.front_clear():
        bit.move()
        if bit.get_color()=='green':
            bit.paint('blue')
        elif bit.get_color()==None:
            bit.paint('blue') 
